{
    "name": "cakephp/app",
    "description": "CakePHP skeleton app",
    "homepage": "http://cakephp.org",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.1",
        "mobiledetect/mobiledetectlib": "2.*",
        "cakephp/migrations": "~1.0",
        "cakephp/plugin-installer": "*",
        "friendsofcake/search": "^1.2",
        "ittvn/Ittvn": "dev-master",
        "ittvn/Templates": "dev-master",
        "ittvn/Users": "dev-master",
        "ittvn/Settings": "dev-master",
        "ittvn/Themes": "dev-master",
        "ittvn/Metas": "dev-master",
        "ittvn/Acl": "dev-master",
        "ittvn/Extensions": "dev-master",
        "ittvn/Contents": "dev-master",
        "ittvn/Medias": "dev-master",
        "ittvn/Menus": "dev-master",
        "ittvn/Blocks": "dev-master",
        "ittvn/Translates": "dev-master",
        "ittvn/ItForms": "dev-master"
    },
    "require-dev": {
        "psy/psysh": "@stable",
        "cakephp/debug_kit": "~3.2",
        "cakephp/bake": "~1.1"
    },
    "suggest": {
        "phpunit/phpunit": "Allows automated tests to be run without system-wide install.",
        "cakephp/cakephp-codesniffer": "Allows to check the code against the coding standards used in CakePHP."
    },
    "autoload": {
        "psr-4": {
            "App\\": "src",
            "BakeTemplates\\": "./plugins/BakeTemplates/src",
            "Sites\\": "./plugins/Sites/src",
            "Products\\": "./plugins/Products/src",
            "Countries\\": "./plugins/Countries/src",
            "Booking\\": "./plugins/Booking/src",
            "School\\": "./plugins/School/src",
            "ItThemes\\": "./plugins/ItThemes/src",
            "NewThemes\\": "./plugins/NewThemes/src",
            "04319459_NewThemes\\": "./plugins/04319459_NewThemes/src",
            "C1psaiNewThemes\\": "./plugins/C1psaiNewThemes/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
            "BakeTemplates\\Test\\": "./plugins/BakeTemplates/tests",
            "Sites\\Test\\": "./plugins/Sites/tests",
            "Products\\Test\\": "./plugins/Products/tests",
            "Countries\\Test\\": "./plugins/Countries/tests",
            "Booking\\Test\\": "./plugins/Booking/tests",
            "School\\Test\\": "./plugins/School/tests",
            "ItThemes\\Test\\": "./plugins/ItThemes/tests",
            "NewThemes\\Test\\": "./plugins/NewThemes/tests",
            "04319459_NewThemes\\Test\\": "./plugins/04319459_NewThemes/tests",
            "C1psaiNewThemes\\Test\\": "./plugins/C1psaiNewThemes/tests"
        }
    },
    "scripts": {
        "post-install-cmd": "App\\Console\\Installer::postInstall",
        "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
